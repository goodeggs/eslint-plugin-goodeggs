{
  "name": "eslint-plugin-goodeggs",
  "version": "12.0.1",
  "author": "Good Eggs Inc.",
  "description": "An eslint plugin containing linter rules specific to Good Eggs.",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib/**/*",
    "prettier-config.js"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "yarn run clean && babel --out-dir lib --copy-files src",
    "clean": "rm -rf lib",
    "fmt": "yarn run lint --fix",
    "lint": "eslint --ext js,jsx,ts,tsx --ignore-path .gitignore --ignore-pattern '!.*rc.js' .",
    "prepublish": "yarn run build",
    "test": "yarn run lint && yarn run test:unit",
    "test:unit": "jest $JEST_FLAGS"
  },
  "dependencies": {
    "@goodeggs/prettier-config": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-problems": "^5.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-flowtype": "^5.3.0",
    "eslint-plugin-goodeggs": "file:.",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^12.1.0",
    "jest": "^27.0.4",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-problems": "^5.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-flowtype": "^5.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-unicorn": "^12.1.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
