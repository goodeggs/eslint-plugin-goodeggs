{
  "name": "eslint-plugin-goodeggs",
  "version": "8.0.0-beta.2",
  "author": "Good Eggs Inc.",
  "description": "An eslint plugin containing linter rules specific to Good Eggs.",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib/**/*",
    "prettier-config.js"
  ],
  "scripts": {
    "build": "yarn run clean && babel --out-dir lib --copy-files src",
    "clean": "rm -rf lib",
    "fmt": "yarn run lint --fix",
    "lint": "eslint --ext js,jsx,ts,tsx --ignore-path .gitignore --ignore-pattern '!.*rc.js' .",
    "prepublish": "yarn run build",
    "test": "yarn run lint && yarn run test:unit",
    "test:unit": "jest $JEST_FLAGS"
  },
  "dependencies": {
    "@goodeggs/prettier-config": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-problems": "^2.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.10.4",
    "eslint-plugin-goodeggs": "file:.",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.9.0",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "jest": "^24.8.0",
    "prettier": "1.17.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-problems": "^2.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.10.4",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.9.0",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "prettier": "1.17.0",
    "typescript": "*"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
